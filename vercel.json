{
    "version": 2,
    "build": {
      "src": "frontoffice/package.json",
      "use": "@vercel/next",
      "commands": [
        "chmod +x deploy.sh",
        "./deploy.sh"
      ]
    },
    "routes": [
      {
        "src": "/backoffice/(.*)",
        "dest": "/backoffice/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontoffice/$1"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "API_URL": "https://hi-interactive-com.vercel.app/"
    }
  }